RewriteEngine on

# picaboo (ios 3) #broken atm
RewriteRule ^auth$ API/picaboo/Auth/Login.php [L]
RewriteRule ^refresh$ API/picaboo/Auth/Refresh.php [L]

# picaboo (ios 4) && snapchat 3.X

#snapchat 4.X
RewriteRule ^ph/login$ API/bq/Auth/Login.php [L]
RewriteRule ^ph/sync$ API/ph/Auth/Sync.php [L]
RewriteRule ^ph/register$ API/ph/Auth/Register.php [L]
RewriteRule ^ph/registeru$ API/bq/Auth/RegisterUsername.php [L]
RewriteRule ^ph/friend$ API/bq/Friends/FriendsHandler.php [L]
RewriteRule ^ph/blob$ API/bq/Snaps/Blob.php [L]
RewriteRule ^ph/upload$ API/bq/Snaps/Upload.php [L]
RewriteRule ^ph/send$ API/bq/Snaps/Send.php [L]


# snapchat 5.X - 6.X
RewriteRule ^bq/login$ API/bq/Auth/Login.php [L]
RewriteRule ^bq/register$ API/bq/Auth/Register.php [L]
RewriteRule ^bq/registeru$ API/bq/Auth/RegisterUsername.php [L]
RewriteRule ^bq/updates$ API/bq/Auth/Refresh.php [L]
RewriteRule ^bq/friend$ API/bq/Friends/FriendsHandler.php [L]
RewriteRule ^bq/upload$ API/bq/Snaps/Upload.php [L]
RewriteRule ^bq/send$ API/bq/Snaps/Send.php [L]
RewriteRule ^bq/retry$ API/bq/Snaps/Retry.php [L]
RewriteRule ^bq/update_snaps$ API/bq/Snaps/UpdateSnaps.php [L]
RewriteRule ^bq/blob$ API/bq/Snaps/Blob.php [L]

